{
  "editor.formatOnSave": true,
  "jest.jestCommandLine": "npm run jest --",
  "jest.autoRun": "off",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[github-actions-workflow]": {
    "editor.autoIndent": "keep",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.rulers": [80, 100, 120],
    "editor.tabSize": 2,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": false
    },
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": false
    }
  },
  "markdown.extension.toc.updateOnSave": false,
  "typescript.format.semicolons": "insert",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[yaml]": {
    "editor.autoIndent": "keep",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.rulers": [80, 100, 120],
    "editor.tabSize": 2,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": false
    },
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "yaml.format.proseWrap": "always",
  "yaml.format.singleQuote": true,
  "yaml.schemas": {
    "./tools/schemas/engine/classes.json": [
      "/content/*/reference/engine/classes/*.yaml",
      "/tools/schemas/engine/examples/class.yaml"
    ],
    "./tools/schemas/engine/datatypes.json": [
      "/content/*/reference/engine/datatypes/*.yaml",
      "/tools/schemas/engine/examples/datatype.yaml"
    ],
    "./tools/schemas/engine/enums.json": [
      "/content/*/reference/engine/enums/*.yaml",
      "/tools/schemas/engine/examples/enum.yaml"
    ],
    "./tools/schemas/engine/globals.json": [
      "/content/*/reference/engine/globals/*.yaml",
      "/tools/schemas/engine/examples/global.yaml"
    ],
    "./tools/schemas/engine/libraries.json": [
      "/content/*/reference/engine/libraries/*.yaml",
      "/tools/schemas/engine/examples/library.yaml"
    ],
    "./tools/schemas/navigation.json": "/content/*/navigation/**/*.yaml"
  },
